{
  "version": 2,
  "builds": [{ "src": "app.py", "use": "@vercel/python" }],
  "routes": [
    {
      "src": "/(.*)",
      "methods": ["GET", "POST", "PUT", "DELETE", "OPTIONS"],
      "headers": "Origin, X-Requested-With, Content-Type, Accept, Authorization",
      "dest": "/app.py",
    }
  ]
}
